<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DonemProje.View.gRPCPage" x:Name="gRPCView" Title="gRPC">
    <ContentPage.Content>
        <StackLayout Spacing="20" Padding="20">
            <Button Text="{Binding BtnText}" FontSize="23" x:Name="btn" 
                    Command="{Binding Path=BindingContext.BtngRPC,
                        Source={x:Reference gRPCView}}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>